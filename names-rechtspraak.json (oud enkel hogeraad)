apper\">\r\n\r\n            <div class=\"metanav\">\r\n\r\n                <ul>\r\n                    <li><a href=\"http:\/\/www.rechtspraak.nl\/servicepaginas\/Pages\/Contact.aspx\">Contact<\/a> | <\/li>\r\n                    <li><a href=\"http:\/\/www.rechtspraak.nl\/sitemap\/Pages\/Sitemap.aspx\">Sitemap<\/a> | <\/li>\r\n                    <li><a href=\"http:\/\/www.rechtspraak.nl\/sitemap\/Pages\/azIndex.aspx\">A-Z index<\/a> | <\/li>\r\n                    <li><a href=\"http:\/\/www.rechtspraak.nl\/servicepaginas\/Pages\/rss.aspx\">RSS<\/a> | <\/li>\r\n                    <li><a href=\"http:\/\/www.rechtspraak.nl\/english\/Pages\/default.aspx\">English<\/a><\/li>\r\n                <\/ul>\r\n                <div class=\"clear\">\r\n                <\/div>\r\n            <\/div>\r\n        <\/div>\r\n                <\/div>\r\n                \r\n                <!-- END HEADER -->\r\n    <div id=\"liquidcontent\">\r\n    <ul class=\"breadcrumb\"><li><a href=\"http:\/\/www.rechtspraak.nl\/Pages\/default.aspx\">Home<\/a><\/li><li><span class=\"separator\">&gt;<\/span><a href=\"http:\/\/www.rechtspraak.nl\/Uitspraken-en-Registers\/Pages\/default.aspx\">Uitspraken en registers<\/a><\/li><li><span class=\"separator\">&gt;<\/span><a href=\"http:\/\/namenlijst.rechtspraak.nl\/\">Nevenfuncties van rechters<\/a><\/li><\/ul>\r\n\r\n    <!--main content-->\r\n    <div id=\"liquid2columncontainer\">\r\n        <div id=\"liquidcentercolumncontainer\">\r\n            <div id=\"liquidcentercolumn\">\r\n                <!-- div reset syle (add stylesheet met reset style) -->\r\n                \r\n<div class=\"tabnavigation\">\r\n    <ul>\r\n        <li><input type=\"submit\" name=\"ctl00$tabControl$tabZoeken\" value=\"zoeken\" id=\"ctl00_tabControl_tabZoeken\" \/><\/li>\r\n        <li><input type=\"submit\" name=\"ctl00$tabControl$tabResultaat\" value=\"resultaat\" id=\"ctl00_tabControl_tabResultaat\" \/><\/li>\r\n        <li class=\"endselected\"><input type=\"submit\" na